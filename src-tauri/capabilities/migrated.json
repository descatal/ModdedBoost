{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "dialog:allow-open",
    "window:default",
    "window:allow-minimize",
    "window:allow-maximize",
    "window:allow-unmaximize",
    "window:allow-close",
    "window:allow-start-dragging",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-clear",
    "store:allow-save",
    "fs:allow-read-text-file",
    {
      "identifier": "fs:scope",
      "allow": [{ "path": "$RESOURCE" }, { "path": "$RESOURCE/**" }]
    }
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ]
}